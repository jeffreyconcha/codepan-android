<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="progressWheel">
        <attr name="progressIndeterminate" format="boolean"/>
        <attr name="barSpinCycleTime" format="integer"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="fillRadius" format="boolean"/>
        <attr name="linearProgress" format="boolean"/>
        <attr name="barColor" format="color"/>
        <attr name="rimColor" format="color"/>
        <attr name="rimWidth" format="dimension"/>
        <attr name="spinSpeed" format="integer"/>
        <attr name="circleColor" format="color"/>
        <attr name="radius" format="dimension"/>
        <attr name="barWidth" format="dimension"/>
        <attr name="barHeight" format="dimension"/>
        <attr name="delayMillis" format="integer"/>
        <attr name="contourColor" format="color"/>
        <attr name="contourSize" format="float"/>
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="progressValue" format="integer"/>
        <attr name="max" format="integer"/>
        <attr name="progressFont" format="string"/>
    </declare-styleable>

    <declare-styleable name="codePan">
        <attr name="typeface" format="string"/>
        <attr name="textColorPressed" format="color"/>
        <attr name="textColorEnabled" format="color"/>
        <attr name="textColorDisabled" format="color"/>
        <attr name="backgroundPressed" format="reference"/>
        <attr name="backgroundEnabled" format="reference"/>
        <attr name="backgroundDisabled" format="reference"/>
        <attr name="enableStatePressed" format="boolean"/>
        <attr name="autoHideKeyboard" format="boolean"/>
        <attr name="autoClearFocus" format="boolean"/>
        <attr name="setScrollable" format="boolean"/>
        <attr name="setSquare" format="boolean"/>
        <attr name="setRequired" format="boolean"/>
        <attr name="reference" format="enum">
            <enum name="width" value="1"/>
            <enum name="height" value="2"/>
            <enum name="min" value="3"/>
            <enum name="max" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="wheelView">
        <attr name="src" format="reference"/>
    </declare-styleable>

    <declare-styleable name="cornerView">
        <attr name="fillColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="inverseCircle">
        <attr name="strokeThickness" format="integer"/>
        <attr name="strokeColor" format="color"/>
        <attr name="edgeThickness" format="integer"/>
        <attr name="color" format="color"/>
    </declare-styleable>

    <declare-styleable name="table">
        <attr name="cellLayouts" format="reference"/>
        <attr name="headerLayout" format="reference"/>
    </declare-styleable>

    <declare-styleable name="InputPinView">
        <attr name="pinLength" format="integer"/>
        <attr name="pinTextSize" format="dimension"/>
        <attr name="pinTextColor" format="color"/>
        <attr name="itemSpacing" format="dimension"/>
        <attr name="itemBackground" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CalendarView">
        <attr name="cal_title" format="string"/>
        <attr name="cal_titleFont" format="string"/>
        <attr name="cal_titleTextSize" format="dimension"/>
        <attr name="cal_titlePadding" format="dimension"/>
        <attr name="cal_accentColor" format="color"/>
        <attr name="cal_selectedColor" format="color"/>
        <attr name="cal_defaultTexColor" format="color"/>
        <attr name="cal_contentFont" format="string"/>
        <attr name="cal_contentPadding" format="dimension"/>
        <attr name="cal_contentTextSize" format="dimension"/>
        <attr name="cal_dateTextSize" format="dimension"/>
        <attr name="cal_yearTextSize" format="dimension"/>
        <attr name="cal_monthTextSize" format="dimension"/>
        <attr name="cal_arrowSize" format="dimension"/>
        <attr name="cal_arrowIconWidth" format="dimension"/>
        <attr name="cal_arrowIconHeight" format="dimension"/>
        <attr name="cal_buttonFont" format="string"/>
        <attr name="cal_buttonWidth" format="dimension"/>
        <attr name="cal_buttonHeight" format="dimension"/>
        <attr name="cal_buttonTextSize" format="dimension"/>
        <attr name="cal_buttonSpacing" format="dimension"/>
        <attr name="cal_buttonPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TimePickerView">
        <attr name="tp_title" format="string"/>
        <attr name="tp_titleFont" format="string"/>
        <attr name="tp_titleTextSize" format="dimension"/>
        <attr name="tp_titlePadding" format="dimension"/>
        <attr name="tp_accentColor" format="color"/>
        <attr name="tp_defaultTexColor" format="color"/>
        <attr name="tp_hourSelectedTextColor" format="color"/>
        <attr name="tp_hourSelectedBackgroundColor" format="color"/>
        <attr name="tp_minuteSelectedTextColor" format="color"/>
        <attr name="tp_minuteSelectedBackgroundColor" format="color"/>
        <attr name="tp_timeTextSize" format="dimension"/>
        <attr name="tp_timeItemWidth" format="dimension"/>
        <attr name="tp_timeItemHeight" format="dimension"/>
        <attr name="tp_timeSpacing" format="dimension"/>
        <attr name="tp_timeUnselectedTextColor" format="color"/>
        <attr name="tp_periodWidth" format="dimension"/>
        <attr name="tp_periodHeight" format="dimension"/>
        <attr name="tp_periodTextSize" format="dimension"/>
        <attr name="tp_periodBorderWidth" format="dimension"/>
        <attr name="tp_periodBorderColor" format="color"/>
        <attr name="tp_periodUnselectedTextColor" format="color"/>
        <attr name="tp_buttonFont" format="string"/>
        <attr name="tp_buttonWidth" format="dimension"/>
        <attr name="tp_buttonHeight" format="dimension"/>
        <attr name="tp_buttonTextSize" format="dimension"/>
        <attr name="tp_buttonSpacing" format="dimension"/>
        <attr name="tp_buttonPadding" format="dimension"/>
        <attr name="tp_colonSize" format="dimension"/>
        <attr name="tp_colonSpacing" format="dimension"/>
    </declare-styleable>

